<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 实现UserDao接口对应的方法 -->
<mapper namespace="com.terren.dao.ILoginDao">
    <!-- 增 -->
    <select id="login" parameterType="com.terren.entity.User" resultType="com.terren.entity.User">
        select * from user where username =#{userName} and password=#{password}
    </select>


</mapper>